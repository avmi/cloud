    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@cpmech/basic": "^2.12.0",
    "chalk": "^2.4.2",
    "clui": "^0.3.6",
    "configstore": "^5.0.0",
    "figlet": "^1.2.4",
